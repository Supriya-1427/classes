const arr=[5,6,7,8,9,10,11,12]
arr.forEach((x)=>{
    console.log()
})
//filter
let filteredArr =arr.filter((x)=>x%2==0)
console.log(filteredArr)

//reduce
let sum=arr.reduce((acc,val)=>acc+val)
console.log(sum)

//slice
let copyArr=arr.slice(2,3+1)
console.log(copyArr)

//splice
arr.splice(2,4)
console.log(arr)
